{
	"name": "pipeline4",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "ForEach1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFileName": "*.json"
						}
					},
					"sink": {
						"type": "AzureSqlSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['array1'][0]['subid']"
								},
								"sink": {
									"name": "c1",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['array1'][0]['array2'][0]['someid']"
								},
								"sink": {
									"name": "c2",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['id']"
								},
								"sink": {
									"name": "c3",
									"type": "Int32"
								}
							}
						],
						"collectionReference": "$['array1'][@pipeline().parameters.temp]['array2']"
					}
				},
				"inputs": [
					{
						"referenceName": "Json2",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlTable3",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "ForEach1",
				"type": "ForEach",
				"dependsOn": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.temp",
						"type": "Expression"
					}
				}
			}
		],
		"parameters": {
			"temp": {
				"type": "array",
				"defaultValue": [
					1,
					2,
					3
				]
			}
		},
		"annotations": []
	}
}